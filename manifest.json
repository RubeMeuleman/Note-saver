{
  "manifest_version": 2,
  "name": "Note saver",
  "description": "Save notes for class",
  "version": "1.0.1",
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "popup.html"
  ]
}